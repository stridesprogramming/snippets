{
"Hugo Partial": {
	"scope": "html,markdown",
	"prefix": "part",
	"body": [
		"{{ partial \"$1.html\" . }}"
	],
	"description": "Make a Hugo Partial"
},

"Hugo Range": {
	"scope": "html,markdown",
	"prefix": "range",
	"body": [
		"{{ range .$1 }}",
		"$2",
		"{{ end }}"
	],
	"description": "Make a Hugo Range"
},

"Hugo Debug Statement": {
	"scope": "html,markdown",
	"prefix": "debug",
	"body": [
		"{{ printf \"%#v\" . }}",
	],
	"description": "Debug current context"
},

"Hugo Variable Template": {
	"scope": "html,markdown",
	"prefix": "var",
	"body": [
		"{{ $$1  := . }}",
	],
	"description": "Hugo variable template"
},

"Hugo If": {
	"scope": "html,markdown",
	"prefix": "if",
	"body": [
		"{{ if .$1 }}",
		"$2",
		"{{ end }}"
	],
	"description": "Make a Hugo Range"
},
}